<h1>Games Today...</h1>

<% @events.each do |event| %>
  <%= event['datetime_local'] %><br/>
  <%= event['title'] %><br/>
  <%= event['url'] %><br/>
  <%= event['venue']['location'] %><br/>
  <br/>
<% end %>


<% if Rails.env.development? %>
  <pre>
    <%= @query.query %>
    <%= @query.attributes %>
    <%= PP.pp(@events, "") %>
  </pre>
<% end %>
